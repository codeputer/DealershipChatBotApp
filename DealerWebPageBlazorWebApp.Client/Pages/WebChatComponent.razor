@page "/webchatClient"
@rendermode InteractiveAuto

<ErrorBoundary>

  <ChildContent>
    <PageTitle>Dealer Page ABC</PageTitle>
    <h3>Web Chat Client</h3>
    <h2>Render mode:@RendererInfo.Name</h2>

    <div>
      <label for="dealertoken">Dealer JTW Token:</label>
      <textarea id="dealertoken" @bind="JWTTokensDTO_Client.DealerJwtToken" style="width: 100%; height: 250px; overflow: auto;"></textarea>
    </div>

    <div>
      <label for="webchattoken">Webchat JTW Token:</label>
      <textarea id="webchattoken" @bind="JWTTokensDTO_Client.WebchatJwtToken" style="width: 100%; height: 250px; overflow: auto;"></textarea>
    </div>

  </ChildContent>
  <ErrorContent>
    @context error
    <p>An error occurred while loading the component.</p>
    <p>@errorMessage</p>
    <pre>@errorStackTrace</pre>
  </ErrorContent>

</ErrorBoundary>



